#pragma config(Sensor, dgtl1,  ,               sensorTouch)
#pragma config(Sensor, dgtl2,  fireButton,     sensorTouch)
#pragma config(Motor,  port2,           motor,         tmotorVex393HighSpeed_MC29, openLoop)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

task main()
{
while(true)
	{
	if (SensorValue(fireButton) == 1) {
		startMotor(port2, 127);
		wait(0.2);
		stopMotor(port2);
		startMotor(port2, -30);
		wait(0.4);
		stopMotor(port2);
	}
	}
}
